{
  "name": "generator-von-neumann",
  "version": "0.1.0",
  "description": "Generator of generators",
  "main": "./generators/index.js",
  "scripts": {
    "prebuild": "run-s -s clean lint",
    "build": "tsc",
    "postbuild": "run-s -s copy",
    "clean": "run-s -s clean:*",
    "clean:output": "rimraf generators",
    "clean:coverage": "rimraf coverage",
    "clean:temp": "rimraf temp",
    "copy": "run-s -s copy:*",
    "copy:templates": "cpx \"src/*/templates/**/*\" generators",
    "copy:with-extensions": "cpx \"src/*/templates/**/.*\" generators",
    "fix": "run-s -s fix:*",
    "fix:eslint": "eslint --fix src/**/*.*",
    "fix:prettier": "prettier -w .",
    "lint": "run-s -s lint:*",
    "lint:eslint": "eslint src/**/*.*",
    "lint:prettier": "prettier -c .",
    "prepack": "run-s -s build",
    "start": "node ./generators/index.js",
    "pretest": "run-s -s build",
    "test": "jest",
    "dev": "npm uninstall -g generator-von-neumann && npm run build && npm i -g ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/rimraf": "^3.0.2",
    "@types/yeoman-generator": "^4.11.4",
    "@types/yeoman-test": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cpx": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "yeoman-test": "^6.1.0"
  },
  "dependencies": {
    "17": "^0.0.0",
    "generator-ts-console": "^1.7.0",
    "yeoman-generator": "^4.13.0"
  },
  "files": [
    "generators"
  ]
}
