{
  "name": "generator-von-neumann",
  "version": "0.0.1",
  "description": "Generator of generators",
  "main": "generators/app/index.js",
  "scripts": {
    "test": "jest",
    "start": "node ./lib/index.js",
    "prebuild": "npm run lint && rm -rf lib/*",
